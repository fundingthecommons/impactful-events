{
  "name": "ftc-t3",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "dev:log": "next dev 2>&1 | tee ftc.log",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:email": "vitest run --grep 'Email'",
    "test:coverage": "vitest run --coverage",
    "test:all": "bun run test:unit && bun run test:integration && bun run test:e2e",
    "test:form": "vitest run --config vitest.config.form.ts src/app/_components/__tests__/core-form-logic.test.ts src/app/_components/__tests__/simple-validation.test.ts",
    "test:form-performance": "vitest run --config vitest.config.form.ts src/app/_components/__tests__/performance.test.tsx",
    "test:form-integration": "vitest run --config vitest.config.form.ts src/app/_components/__tests__/form-integration.test.tsx",
    "test:form-scroll": "vitest run --config vitest.config.form.ts src/app/_components/__tests__/scroll-behavior.test.tsx",
    "test:form-validation": "vitest run --config vitest.config.form.ts src/app/_components/__tests__/form-validation.test.tsx",
    "test:form-watch": "vitest watch --config vitest.config.form.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@mantine/core": "8.1.1",
    "@mantine/dates": "^8.2.5",
    "@mantine/form": "^8.2.5",
    "@mantine/hooks": "8.1.1",
    "@mantine/notifications": "^8.2.5",
    "@notionhq/client": "^4.0.0",
    "@prisma/client": "^6.5.0",
    "@react-email/components": "^0.5.1",
    "@react-email/render": "^1.2.1",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tabler/icons-react": "^3.34.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "bcryptjs": "^3.0.2",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "googleapis": "^150.0.1",
    "mantine-form-zod-resolver": "^1.3.0",
    "next": "^15.2.3",
    "next-auth": "5.0.0-beta.25",
    "postmark": "^4.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.0.15",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "msw": "^2.10.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.5.0",
    "tailwindcss": "^4.0.15",
    "tsx": "^4.19.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  }
}
